    "express": "^4.17.0",
    "express-promise-router": "^3.0.3",
    "helmet": "^3.18.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.17"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.43",
